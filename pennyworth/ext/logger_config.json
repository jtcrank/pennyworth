{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(levelname)s - [%(name)s]: %(message)s"
        }
    },
    "handlers": {
        "file_handler": {
            "class": "logging.handlers.LogFileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "log_0"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": ["file_handler"]
    }
}
